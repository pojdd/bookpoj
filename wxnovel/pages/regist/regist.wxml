<!--pages/regist/regist.wxml-->
<view class="container">
  <input animation="{{slide_ac}}" placeholder="请输入账号" bindinput="getac" maxlength='10'></input>
  <input animation="{{slide_pw}}" placeholder="请输入密码" bindinput="getpw" type="password" maxlength='24'></input>
  <input animation="{{slide_email}}" placeholder="请输入邮箱" bindchange="getemail" maxlength='24'></input>
  <button class="btn" bindtap="btn">注册</button>
</view>
