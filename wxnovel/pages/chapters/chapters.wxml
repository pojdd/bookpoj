<!--pages/chapters/chapters.wxml-->
<view wx:for="{{chapters}}" class="chapterframe" 
  bindtap="getDetail" data-chapterindex="{{index}}">
  <view class="chapter"
  bindtap="" data-chapterindex="{{index}}">
    <text class="text"bindtap="" 
    data-chapterindex="{{index}}">{{chapters[index]}}</text>
  </view>
</view>
