<!--pages/bookshelf/bookshelf.wxml-->
<image class="shelftop" src="../../iconimg/backtap2.png">
</image>
<view class="shelfbody">
  <view class="shelfitem" wx:for="{{bookshelf}}">
    <image src="{{item.pic}}" class="item-image"></image>
    <text class="item-text">{{item.bookname}}</text>
  </view>
</view>

